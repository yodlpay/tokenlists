    "jest": "29",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsdx": "^0.14.1",
    "tsx": "^4.15.6",
    "typescript": "^4.8.2",
    "viem": "^2.15.1"
